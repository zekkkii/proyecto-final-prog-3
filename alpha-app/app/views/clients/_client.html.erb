<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= client.last_name %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= client.phone_number %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

</div>
